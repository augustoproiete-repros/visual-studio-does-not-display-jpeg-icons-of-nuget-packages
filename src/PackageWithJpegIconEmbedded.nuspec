<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>PackageWithJpegIconEmbedded</id>
    <version>1.0.0</version>
    <authors>augustoproiete</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <icon>images\icon.jpeg</icon>
    <projectUrl>https://github.com/augustoproiete-repros/visual-studio-does-not-display-jpeg-icons-of-nuget-packages</projectUrl>
    <repository type="git" url="https://github.com/augustoproiete-repros/visual-studio-does-not-display-jpeg-icons-of-nuget-packages.git" />
    <description>NuGet Package that uses a JPEG icon embedded</description>
    <dependencies>
      <group targetFramework=".NETStandard2.0" />
    </dependencies>
  </metadata>
  <files>
    <file src="..\asset\icon.jpeg" target="images\icon.jpeg" />
  </files>
</package>
